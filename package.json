{
  "name": "print-middleware",
  "version": "1.1.0",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsup && pkg ."
  },
  "bin": "dist/server.js",
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^20.0.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0"
  }
}
